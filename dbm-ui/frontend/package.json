    "@blueking/date-picker": "1.0.1",
    "@blueking/ip-selector": "0.3.0-beta.23",
    "@blueking/login-modal": "^1.0.5",
    "@blueking/notice-component": "2.0.5",
    "@blueking/sub-saas": "0.0.0-beta.6",
    "@blueking/table": "^0.0.1-beta.39",
    "@icon-cool/bk-icon-bk-biz-components": "^0.0.4",
    "@vueuse/core": "^13.0.0",
    "axios": "^1.8.4",
    "@xterm/xterm": "^5.5.0",
    "bkui-vue": "2.0.1-beta.104",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "html-to-image": "^1.11.13",
    "js-cookie": "3.0.5",
    "jsencrypt": "3.3.2",
    "lodash": "4.17.21",
    "mitt": "3.0.1",
    "monaco-editor": "^0.52.0",
    "pinia": "^3.0.1",
    "qs": "^6.14.0",
    "screenfull": "6.0.2",
    "sql-formatter": "^15.4.11",
    "tiny-pinyin": "1.3.2",
    "tippy.js": "6.3.7",
    "url-join": "5.0.0",
    "vue": "^3.5.13",
    "vue-content-loader": "2.0.1",
    "vue-i18n": "^11.1.2",
    "vue-request": "2.0.4",
    "vue-router": "^4.5.0",
    "vue-types": "^6.0.0",
    "vuedraggable": "4.1.0",
    "xlsx": "^0.18.5",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0"
