    "@blueking/ip-selector": "0.3.0-beta.23",
    "@blueking/login-modal": "1.0.5",
    "@blueking/notice-component": "2.0.5",
    "@blueking/sub-saas": "0.0.0-beta.6",
    "@blueking/table": "0.0.1-beta.39",
    "@blueking/tdesign-ui": "0.0.1-beta.31",
    "@icon-cool/bk-icon-bk-biz-components": "0.0.4",
    "@vueuse/core": "13.4.0",
    "@xterm/addon-fit": "0.10.0",
    "@xterm/addon-web-links": "0.11.0",
    "@xterm/xterm": "5.5.0",
    "axios": "1.10.0",
    "bkui-vue": "2.0.2-beta.40",
    "date-fns": "4.1.0",
    "dayjs": "1.11.13",
    "echarts": "5.6.0",
    "html-to-image": "1.11.13",
    "js-cookie": "3.0.5",
    "jsencrypt": "3.3.2",
    "lodash": "4.17.21",
    "mitt": "3.0.1",
    "monaco-editor": "0.52.2",
    "pinia": "3.0.3",
    "qs": "6.14.0",
    "screenfull": "6.0.2",
    "sql-formatter": "^15.6.6",
    "tiny-pinyin": "1.3.2",
    "tippy.js": "6.3.7",
    "url-join": "5.0.0",
    "vue": "3.5.17",
    "vue-content-loader": "2.0.1",
    "vue-i18n": "11.1.7",
    "vue-request": "2.0.4",
    "vue-router": "4.5.1",
    "vue-types": "6.0.0",
    "vuedraggable": "4.1.0",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "19.8.1",
    "@prettier/plugin-oxc": "0.0.4",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@tsconfig/node22": "22.0.2",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.0.10",
    "@types/qs": "6.14.0",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-vue": "6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.5.1",
    "@vue/tsconfig": "0.7.0",
    "commitlint": "19.8.1",
    "cross-env": "7.0.3",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-config-tencent": "1.1.3",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-oxlint": "^1.5.0",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-vue": "10.2.0",
    "globals": "^16.3.0",
    "less": "4.3.0",
    "lint-staged": "16.1.2",
    "npm-run-all2": "8.0.4",
    "oxlint": "^1.5.0",
    "postcss-html": "1.8.0",
    "postcss-less": "6.0.0",
    "prettier": "^3.6.2",
    "simple-git-hooks": "2.13.0",
    "stylelint": "16.21.0",
    "stylelint-config-standard": "38.0.0",
    "stylelint-less": "3.0.1",
    "stylelint-order": "7.0.0",
    "typescript": "5.8.3",
    "unplugin-auto-import": "19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "7.0.0",
    "vite-plugin-babel": "^1.3.2",
    "vite-plugin-imp": "2.4.0",
    "vite-plugin-monaco-editor": "1.1.0",
    "vite-plugin-static-copy": "^3.1.0",
    "vite-plugin-style-import": "2.0.0",
    "vue-component-type-helpers": "^3.0.0",
    "vue-tsc": "^3.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "cd dbm-ui/frontend && npx lint-staged",
    "commit-msg": "cd dbm-ui/frontend && npx commitlint --edit $1"
  },
  "lint-staged": {
    "*.css": "stylelint --allow-empty-input",
    "*.less": "stylelint --allow-empty-input --fix --customSyntax=postcss-less",
    "*.{js,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.1"
  },
  "overrides": {
    "@rolldown/pluginutils": "1.0.0-beta.21"
  },
  "resolutions": {
    "@rolldown/pluginutils": "1.0.0-beta.21"
  }
}
