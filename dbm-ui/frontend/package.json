  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/node22": "^22.0.1",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.15",
    "@types/qs": "^6.9.18",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "commitlint": "^19.8.0",
    "cross-env": "7.0.3",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-config-tencent": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-oxlint": "^0.16.4",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "less": "^4.2.2",
    "lint-staged": "^15.5.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.16.4",
    "postcss-html": "^1.8.0",
    "postcss-less": "6.0.0",
    "prettier": "^3.5.3",
    "simple-git-hooks": "^2.12.1",
    "stylelint": "^16.17.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-less": "3.0.1",
    "stylelint-order": "6.0.4",
    "typescript": "^5.8.2",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.4",
    "vite-plugin-babel": "^1.3.0",
    "vite-plugin-imp": "2.4.0",
    "vite-plugin-monaco-editor": "1.1.0",
    "vite-plugin-static-copy": "^2.3.0",
    "vite-plugin-style-import": "2.0.0",
    "vue-component-type-helpers": "^2.2.8",
    "vue-tsc": "^2.2.8"
  },
  "simple-git-hooks": {
    "pre-commit": "cd dbm-ui/frontend && npx lint-staged",
    "commit-msg": "cd dbm-ui/frontend && npx commitlint --edit $1"
  },
  "lint-staged": {
    "*.css": "stylelint --allow-empty-input",
    "*.less": "stylelint --allow-empty-input --fix --customSyntax=postcss-less",
    "*.{js,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.1"
  }
}
